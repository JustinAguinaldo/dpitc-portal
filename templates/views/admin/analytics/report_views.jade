extends ../../../layouts/admin-default

block content

    .col-sm-9.col-sm-offset-3.col-md-10.col-md-offset-2.main
        .table-responsive   
            table.table.table-striped
                thead
                tr
                    th ID
                    th ip
                    th Time
                tbody
                each view in locals.data.report_views
                    tr   
                        td 
                        td #{view.ip}
                        td #{view.time}
                else
                    .jumbotron
                        h3 No data